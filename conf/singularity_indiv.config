singularity {
    enabled = true
}

process {
    withLabel: genometools {
        container = "containers/singularity/genometools.sif"
    }
    withLabel: python3 {
        container = "containers/singularity/python3.sif"
    }
    withLabel: posix {
        container = "containers/singularity/posix.sif"
    }
    withLabel: download {
        container = "containers/singularity/posix.sif"
    }
    withLabel: repeatmasker {
        container = "containers/singularity/repeatmasker.sif"
    }
    withLabel: aragorn {
        container = "containers/singularity/aragorn.sif"
    }
    withLabel: trnascan {
        container = "containers/singularity/trnascan.sif"
    }
    withLabel: infernal {
        container = "containers/singularity/infernal.sif"
    }
    withLabel: structrnafinder {
        container = "containers/singularity/structrnafinder.sif"
    }
    withLabel: rnammer {
        container = "containers/singularity/rnammer.sif"
    }
    withLabel: gffpal {
        container = "containers/singularity/python3.sif"
    }
}

